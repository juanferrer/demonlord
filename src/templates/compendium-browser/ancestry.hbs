<li class="item flexrow" data-entry-uuid="{{entry.uuid}}" data-type="Item">
    <div class="image">
        <img src="{{entry.img}}" alt="{{entry.name}}" loading="lazy" />
    </div>
    <div class="name">
        <a class="item-link">{{entry.name}}</a>
    </div>

    <div class="tags">

    </div>

    <div class="level">
        <span data-tippy-content="{{localize DL.SpellRank}}">{{entry.level}}</span>
    </div>
</li>